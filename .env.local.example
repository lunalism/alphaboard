# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Site URL (for OAuth redirect)
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Beta Mode (closed = 화이트리스트 기반 접근 제어, open = 누구나 접근 가능)
NEXT_PUBLIC_BETA_MODE=closed

# Brandfetch API (https://brandfetch.com)
BRANDFETCH_API_KEY=your_brandfetch_api_key

# Brandfetch Logo Link API (https://brandfetch.com/developers)
NEXT_PUBLIC_BRANDFETCH_CLIENT_ID=your_brandfetch_client_id

# Cloudinary (이미지 업로드용)
# https://cloudinary.com/console
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret
